<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="application"
		class="org.dayatang.springtest.application.impl.MyApplicationImpl">
	</bean>

	<bean id="customBean" class="org.dayatang.springtest.test.CustomBean"
		lazy-init="true">
	</bean>

	<bean id="customBean2" class="org.dayatang.springtest.test.CustomBean2"
		lazy-init="false">
	</bean>
	
</beans>